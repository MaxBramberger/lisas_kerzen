{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 100,
    "trailingComma": "all",
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "strict",
    "embeddedLanguageFormatting": "auto",
    "overrides": [
        {
          "files": "*.scss",
          "options": {
            "parser": "css"
          }
        }
      ]
  }